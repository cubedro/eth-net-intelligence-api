[
  {
    "name"              : "node-app",
    "script"            : "app.js",
    "log_date_format"   : "YYYY-MM-DD HH:mm Z",
    "merge_logs"        : false,
    "watch"             : false,
    "max_restarts"      : 10,
    "exec_interpreter"  : "node",
    "exec_mode"         : "fork_mode",
    "env":
    {
      "NODE_ENV"        : "production",
      "RPC_HOST"        : "localhost",
      "RPC_PORT"        : "8545",
      "LISTENING_PORT"  : "30303",
      "INSTANCE_NAME"   : "MyNodename",
      "CONTACT_DETAILS" : "info@MyDomain",
      "WS_SERVER"       : "wss://rpc.ethstats.net",
      "WS_SECRET"       : "XXYYYYYZZZZZZ",
      "VERBOSITY"       : 2
    }
  }
]
